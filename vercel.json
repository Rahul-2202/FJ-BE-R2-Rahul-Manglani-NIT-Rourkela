{
  "version": 2,
  "builds": [
    {
      "src": "Expense_Tracker/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    },
#    {
#     "src": "build_files.sh",
#      "use": "@vercel/static-build",
#     "config": {
#        "distDir": "staticfiles"
#     }
#    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Expense_Tracker/wsgi.py"
    }
  ]
}
